{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": 'script',
  },
  "env": {
    "node": "true",
    "mocha": "true"
  },
  "rules": {
    'arrow-parens': [2, "as-needed"],
    'comma-dangle': [2, "never"],
    'object-curly-spacing': [2, 'never'],
    'object-property-newline': [0, {
      'allowMultiplePropertiesPerLine': true,
      }],
  }
}
